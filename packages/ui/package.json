{
	"name": "@crossbell/ui",
	"version": "0.0.28",
	"license": "MIT",
	"repository": "github:Crossbell-Box/crossbell-universe",
	"bugs": "https://github.com/Crossbell-Box/crossbell-universe/issues",
	"homepage": "https://github.com/Crossbell-Box/crossbell-universe#readme",
	"author": "Natural Selection Labs",
	"keywords": [],
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"typings": "dist/index.d.ts",
	"sideEffects": false,
	"files": [
		"dist",
		"package.json"
	],
	"scripts": {
		"build": "run-s clean build:icons build:ts",
		"build:icons": "svgr --typescript --ref --icon --filename-case kebab --index-template svgr-index-template.js --out-dir src/icons/svgr  ../../scripts/unocss/icons",
		"build:ts": "tsup",
		"clean": "rm -rf ./dist"
	},
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./package.json": "./package.json"
	},
	"dependencies": {
		"classnames": "^2",
		"facepaint": "^1.2.1",
		"react-intersection-observer": "9.4.1",
		"react-transition-group": "4.4.5"
	},
	"peerDependencies": {
		"@crossbell/indexer": "workspace:*",
		"@crossbell/util-hooks": "workspace:*",
		"@crossbell/util-metadata": "workspace:*",
		"@tanstack/react-query": "*",
		"crossbell.js": "^0.16",
		"react": "18.x.x",
		"react-dom": "18.x.x"
	},
	"devDependencies": {
		"@crossbell/indexer": "workspace:*",
		"@crossbell/util-hooks": "workspace:*",
		"@crossbell/util-metadata": "workspace:*",
		"@svgr/cli": "6.5.1",
		"@types/facepaint": "^1.2.1",
		"@types/react-transition-group": "^4",
		"pascal-case": "^3.1.2",
		"ts-essentials": "9.3.0"
	}
}
